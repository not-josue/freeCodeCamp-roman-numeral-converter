{
  "name": "roman-numeral-converter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "css-deploy": "npm run css-build && npm run css-postcss",
    "css-build": "sass --no-source-map mystyles.scss:mystyles.css",
    "css-postcss": "postcss --use autoprefixer --output mystyles.css mystyles.css"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "bulma": "^0.9.4",
    "postcss-cli": "^10.1.0",
    "sass": "^1.64.1",
    "vite": "^4.4.5"
  }
}
